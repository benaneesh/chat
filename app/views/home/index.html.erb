<h1>Chatrooms</h1>
<table class="table table-striped">
  <thead>
    <tr>
      <th>ID</th>
      <th>Name</th>
      <th>Created at</th>
    </tr>
  </thead>
  <tbody>
    <% @chatrooms.each do |chatroom| %>
      <tr>
        <td><%= chatroom.id %></td>
        <td><%= link_to chatroom.name, chatroom_path(chatroom) %></td>
        <td><%= chatroom.created_at %></td>
      </tr>
    <% end %>
  </tbody>
</table>